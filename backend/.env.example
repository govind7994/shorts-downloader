# Environment Variables
PORT=3000
NODE_ENV=development

# Rate Limiting
RATE_LIMIT_WINDOW_MS=900000
RATE_LIMIT_MAX_REQUESTS=10

# YouTube Cookies (Required to bypass bot detection)
# Get these from your browser after logging into YouTube
# Format: [{"name":"cookie_name","value":"cookie_value","domain":".youtube.com"}]
YOUTUBE_COOKIES=[]
